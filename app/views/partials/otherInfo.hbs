<p class="lead" style="padding-top: 10px; padding-bottom: 10px">
    {{{otherInfo.heading}}}
</p>
<!--Customer API-->
<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    {{#each otherInfo.info}}
        <div class="panel panel-default">
            <div class="panel-heading" role="tab">
                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{id}}" aria-expanded="true" aria-controls="{{id}}">
                    <h3 class="panel-title">{{title}}</h3>
                </a>
            </div>
            <div id="{{id}}" class="panel-collapse collapse in" role="tabpanel">
                <div class="panel-body">
{{{content}}}
                </div>
            </div>
        </div>
    {{/each}}
</div>